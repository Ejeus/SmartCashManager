        <!--android:onClick="Generate"-->
        <!--android:layout_below="@+id/spinner"-->
        <!--android:layout_centerHorizontal="true"-->
        <!--android:layout_marginTop="34dp" />-->


    </RelativeLayout>
</android.support.constraint.ConstraintLayout>
